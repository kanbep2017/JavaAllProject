package pack;

public class Student {
	private String name;
	private int engScore;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getScore() {
		return engScore;
	}

	public void setScore(int score) {
		if (0 <= score && score <= 100) {
			this.engScore = score;
		} else {
			this.engScore = 0;
		}
	}

}
