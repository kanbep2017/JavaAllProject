package pack;

public class Student {
	private String name;
	private int engscore;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getScore() {
		return engscore;
	}

	public void setScore(int score) {
		if (0 <= score && score <= 100) {
			this.engscore = score;
		} else {
			this.engscore = 0;
		}
	}

}
